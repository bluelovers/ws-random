{
  "name": "random-extra",
  "version": "4.0.1",
  "description": "Seedable random number generator supporting many common distributions.",
  "keywords": [
    "alea",
    "arr",
    "array",
    "bates",
    "d3-random",
    "distribution",
    "el",
    "element",
    "exponential",
    "fisher",
    "gaussian",
    "generate",
    "generate-random-numbers",
    "generator",
    "irwinhall",
    "item",
    "list",
    "lognormal",
    "math",
    "n-random-numbers",
    "normal",
    "number",
    "numbers",
    "pick",
    "poisson",
    "prng",
    "probability",
    "pseudorandom",
    "rand",
    "random",
    "random-extra",
    "random-sum",
    "randomSum",
    "randomize",
    "randomnumbers",
    "rng",
    "seed",
    "seedrandom",
    "select",
    "shuffle",
    "sort",
    "srand",
    "stats",
    "sum",
    "tychei",
    "uniform",
    "uuid",
    "weight",
    "weighted",
    "xor128",
    "xor4096",
    "xorshift7",
    "xorwow",
    "yates",
    "create-by-yarn-tool"
  ],
  "homepage": "https://github.com/bluelovers/ws-random/tree/master/packages/random-extra#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-random/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-random.git"
  },
  "license": "MIT",
  "author": "Travis Fischer <travis@automagical.ai>",
  "main": "index.js",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "pretest_": "yarn run build",
    "test": "jest --maxConcurrency=1 --maxWorkers=1",
    "posttest": "git commit -m \"build: update seed\" ./src/util/random-seed-str.data.* & echo posttest",
    "build_": "yarn run build:index",
    "build:index": "ts-node ./test/script/build-cache.ts",
    "build-minify": "npx -r fast-glob -r fs-extra -r uglify-es -r ts-node ts-node ./test/script/minify",
    "postbuild": "ynpx build-ts-file -- ../@lazy-random/seed-data/src/random-seed-str.data.ts && git add ./src/util/random-seed-str.data.ts ./src/util/random-seed-str.data.js",
    "preversion": "yarn run test",
    "version": "echo version",
    "postversion": "echo postversion",
    "prepublish": "yarn run postpublishOnly:add",
    "prepublishOnly": "echo prepublishOnly",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "publish": "echo publish",
    "postpublish": "echo postpublish",
    "postpublishOnly": "echo postpublishOnly",
    "postpublishOnly:add": "yarn-tool root run postpublishOnly:add random-extra",
    "benchmark": "npx ts-node ./test/benchmark/simple.perf.ts && npx ts-node ./test/benchmark/base.perf.ts && npx ts-node ./test/benchmark/random-id.perf.ts",
    "ncu": "ynpx --quiet yarn-tool -- ncu -u",
    "sort-package-json": "ynpx --quiet yarn-tool -- sort",
    "tsc:showConfig": "ynpx get-current-tsconfig -p"
  },
  "dependencies": {
    "@lazy-random/clone-class": "^1.0.6",
    "@lazy-random/distributions": "^1.0.3",
    "@lazy-random/expect": "^1.0.5",
    "@lazy-random/rng-factory": "^1.0.3",
    "@types/seedrandom": "=2.4.28",
    "chai": "^4",
    "chai-asserttype-extra": "^1.2.3",
    "core-decorators": "^0.20.0",
    "lib-r-math.js": "^1.0.89",
    "num-in-delta": "^1.0.7",
    "num-is-zero": "^1.0.8",
    "ts-type": "^2.1.2",
    "uni-string": "^1.1.11"
  },
  "gitHead": "e61d528c84bd3340a59f288f65d20bfb2906c905",
  "packageManager": "yarn@^1.22.11"
}
